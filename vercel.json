{
  "buildCommand": "cd ject && npm install && npm run build",
  "outputDirectory": "ject/dist",
  "regions": ["iad1"],
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
