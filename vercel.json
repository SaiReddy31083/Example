{
  "buildCommand": "cd ject && npm install && npm run build",
  "outputDirectory": "ject/dist",
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

